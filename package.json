    "jest-css-modules": "^2.0.0",
    "jest-transform-css": "^2.0.0",
    "sinon": "^7.3.1",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "axios": "^0.18.0",
